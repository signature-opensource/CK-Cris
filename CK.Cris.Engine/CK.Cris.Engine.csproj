<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>CK.Setup.Cris</RootNamespace>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="CK.StObj.Runtime" Version="19.0.1--0015-develop" />
    <!--
          This CK.Cris.Engine directly implements the CK.Cris.TypeSript.Engine to avoid another (quite small) assembly.

          Note that Cris TypeScript generation requires:
          - the TypeScriptAspect is declared and one or more <TypeScript><OutuptPath>...</OutuptPath></TypeScript> is defined in the BinPathConfiguration.
          - AND the CK.Cris.TypeScript must be referenced.
    -->
    <PackageReference Include="CK.StObj.TypeScript.Engine" Version="0.7.1--0008-develop" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\CK.Cris\CK.Cris.csproj" />
  </ItemGroup>
</Project>